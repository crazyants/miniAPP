<view class="container splash-container">
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block a:for="{{items}}">
      <swiper-item>
        <image src="{{item}}" mode="aspectFill" class="splash-img"/>
      </swiper-item>
    </block>
  </swiper>
</view>